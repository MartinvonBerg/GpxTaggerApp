{
    "name": "gpx-tagger",
    "version": "1.0.0",
    "description": "An Electron application to add GPX metadata to image files",
    "main": "main.js",
    "author": "Dr. Martin von Berg",
    "license": "GPL-2.0-only",
    "scripts": {
        "start": "electron .",
        "build": "webpack --mode development",
        "webprod": "webpack --mode production",
        "all": "npm run build && npm start",
        "debug": "electron --inspect=9229 .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "package": "npm run webprod && npx electron-packager . GpxTaggerApp --platform=win32 --arch=x64 --out=dist --overwrite --no-prune"
    },
    "devDependencies": {
        "@babel/core": "*",
        "@babel/preset-env": "*",
        "babel-loader": "*",
        "css-loader": "^7.1.2",
        "electron": "^38.1.0",
        "@electron/packager": "*",
        "style-loader": "^4.0.0",
        "webpack": "*",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "@photostructure/tz-lookup": "^11.2.1",
        "@turf/helpers": "^7.2.0",
        "awesomplete": "^1.1.7",
        "coordinate-parser": "^1.0.7",
        "exiftool-vendored": "^30.5.0",
        "geo-tz": "^8.1.4",
        "i18next": "^25.5.2",
        "i18next-fs-backend": "^2.6.0",
        "leaflet": "^1.9.4",
        "leaflet-control-geocoder": "^3.3.1",
        "leaflet-gpx": "^2.2.0",
        "leaflet.locatecontrol": "^0.85.1",
        "sanitize-html": "^2.17.0",
        "sharp": "^0.34.4",
        "ts-loader": "^9.5.4",
        "tslib": "^2.8.1"
    }
}
